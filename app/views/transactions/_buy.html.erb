<%= form_with(url: transactions_buy_stock_path(id: @quote.symbol), method: :post) do |f| %>
  <div class="form-group">
    <%= f.label :quantity, "Quantity", class: 'text-white' %>
    <%= f.number_field :quantity, class: "form-control" %>
     <%= f.submit "Buy Stock", class: "btn-main" %>
  </div>
<% end %>
